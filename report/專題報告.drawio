        </mxCell>
        <mxCell id="UI97_lbdpvDw-j3Kv0Oo-7" value="指令傳輸" style="endArrow=classic;html=1;" edge="1" parent="1" source="UI97_lbdpvDw-j3Kv0Oo-6" target="UI97_lbdpvDw-j3Kv0Oo-8">
          <mxGeometry width="50" height="50" relative="1" as="geometry">
            <mxPoint x="165" y="420" as="sourcePoint" />
